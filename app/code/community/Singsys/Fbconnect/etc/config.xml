<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Singsys_Fbconnect>
            <version>0.1.0</version>
        </Singsys_Fbconnect>
    </modules>
    <default>
        <customer>
            <singsys_fbconnect_facebook>
                <enabled>0</enabled>
            </singsys_fbconnect_facebook>	      	    
        </customer>
    </default>
    <frontend>       
        <layout>
            <updates>
                <singsys_fbconnect>
                    <file>singsys_fbconnect.xml</file>
                </singsys_fbconnect>
            </updates>
        </layout>
    </frontend>    
    <global>
        <models>
            <singsys_fbconnect>
                <class>Singsys_Fbconnect_Model</class>
            </singsys_fbconnect>
        </models>
        <blocks>
            <singsys_fbconnect>
                <class>Singsys_Fbconnect_Block</class>
            </singsys_fbconnect>            
        </blocks>
        <helpers>
            <singsys_fbconnect>
                <class>Singsys_Fbconnect_Helper</class>
            </singsys_fbconnect>
        </helpers>
        <resources>
            <singsys_fbconnect_setup>
                <setup>
                    <module>Singsys_Fbconnect</module>
                    <class>Singsys_Fbconnect_Model_Resource_Setup</class>
                </setup>
            </singsys_fbconnect_setup>
        </resources>        
    </global>
    <frontend>
        <routers>
            <singsys_fbconnect>
                <use>standard</use>
                <args>
                    <module>Singsys_Fbconnect</module>
                    <frontName>fbconnect</frontName>
                </args>
            </singsys_fbconnect>
        </routers>  
    </frontend>    
</config>
